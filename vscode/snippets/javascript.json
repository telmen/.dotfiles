{
	/*
	// Place your snippets for JavaScript here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
	"Print to console": {
		"prefix": "log",
		"body": ["console.log($1)"],
		"description": "Log output to console"
	},
	"Destruct object": {
		"prefix": "obd",
		"body": ["const {$2} = $1"],
		"description": "Destruct value from object with given keys"
	},
	"Arrow function": {
		"prefix": "arf",
		"body": ["$1 = () => { \n $2 \n}"],
		"description": "Arrow function snippet"
	},
	"Background color CSS-in-JS": {
		"prefix": "bgc",
		"body": ["backgroundColor: $1"],
		"description": "backgroundcolor"
	},
	"Create Class bind method": {
		"prefix": "cbind",
		"body": ["this.$1 = this.$1.bind(this)"]
	},
	"React create ref": {
		"prefix": "rref",
		"body": ["this.$1 = React.createRef()"]
	},
	"React isMounted": {
		"prefix": "isMo",
		"body": ["this._isMounted = true"]
	},
	"Export default as": {
		"prefix": "exas",
		"body": ["export { default as $1 } from './$1'"]
	},
	"Simple function es5": {
		"prefix": "fn",
		"body": ["function $1($2) {$3}"]
	},
	"Export default function React": {
		"prefix": "exdfn",
		"body": ["export default function $1($2) {$3}"]
	},
	"Autobind for class methods": {
		"prefix": "abind",
		"body": "import autobind from 'auto-bind'"
	},
	"lendpinoy": {
		"prefix": "lend",
		"body": ["import $2 from '$1'"]
	},
	"Jest mock for npm packages": {
		"prefix": "jmock",
		"body": ["jest.mock('$1', () => { \n\treturn { \t\t\n $2 \n\t} \n})"]
	}
}
