export RUBY_GEM=/usr/lib/ruby/gems/2.3.0/bin
export MANPATH="/usr/local/man:$MANPATH"
export MANPAGER='less -X'
export LANG=en_US.UTF-8
export SSH_KEY_PATH="~/.ssh/rsa_id"
export REACT_EDITOR=vim
export LC_CTYPE=en_US.UTF-8
export G_ENABLE_DIAGNOSTIC=1
export GTK_THEME=Adwaita:dark
export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_151.jdk/Contents/Home
export ANDROID_HOME=$HOME/Library/Android/sdk
export OpenCV_ANDROID_HOME=$ANDROID_HOME/opencv
export PYTHON_HOME=$HOME/Library/Python/3.7/bin
export ANDROID_BIN=$ANDROID_HOME/tools/bin/
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced
export APPS=(yarn global bin):$OpenCV_ANDROID_HOME:./vendor/bin:./node_modules/.bin/:$ANDROID_HOME:$ANDROID_HOME/platform-tools:$ANDROID_BIN:$PYTHON_HOME:$ANDROID_HOME/emulator:$ANDROID_HOME/tools:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/go/bin
export YARN="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin"
export OPEN_SSL="/usr/local/opt/openssl/bin:$PATH"
export RUBY="/usr/local/opt/ruby/bin"
export RVM="$HOME/.rvm/bin"
export PATH="$PATH:$YARN:$OPEN_SSL:$RUBY:$RUBY_GEM:$RVM:$APPS"
